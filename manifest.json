{
    "manifest_version": 2,

    "name": "Study Block",
    "description": "Study... Or Else",
    "version": "1.0",
    "permissions": [
        "tabs",
        "notifications",
        "webNavigation",
        "storage",
        "alarms",
        "https://ajax.googleapis.com/"
    ],
    "background": {
        "scripts": ["js/jquery-3.1.1.min.js", "js/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}